<div class="jumbotron">
    <h1 class="text-center">All Audit Logs</h1>
</div>

<table class="table table-striped">
	<thead>
		<tr>
			<th>
				#
			</th>
      <th>
        Employee
      </th>
			<th>
				Week Starting
			</th>
			<th>
				Confirmed At
			</th>
			<th>
				Status
			</th>
		</tr>
	</thead>
	<tbody id="audit_logs">
    <%= render @audit_logs %>
	</tbody>
</table>

<div class="row justify-content-center" id="paginator">
  <%= paginate @audit_logs, remote: true %>
</div>
