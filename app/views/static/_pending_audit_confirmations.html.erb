<% pending_audit_confirmations.each do |audit| %>
  <%= link_to "Confirm that there is no overtime for the week of #{audit.start_date.strftime("%B %d, %Y")} to #{audit.start_date.strftime("%B %d, %Y") + 6.days}", confirm_audit_log_path(audit), class: "btn btn-primary btn-lg btn-employee-home", id: "confirm_#{audit.id}",
  data:  {confirm: "Are you sure you did not perform overtime?" } %>
<% end %>
